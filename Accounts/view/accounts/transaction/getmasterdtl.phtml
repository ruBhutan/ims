?>
<?php if($this->type_ID == '8'):?>
    <option value=""></option>
    <?php foreach ($this->masterDtls as $mastDtl): ?>
            <option value="<?php echo $mastDtl['id'].'|'.$mastDtl['sub_head'].'|'.$mastDtl['type']; ?>">
