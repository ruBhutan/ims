            </option>
    <?php endforeach; ?>
<?php else: ?>
    <option value=""></option>
    <?php foreach ($this->masterDtls as $mastDtl):
        $sub_head_name = $this->service->getDatabyParam('accounts_sub_head',array('id' => $mastDtl['sub_head']),'name');
        ?>
            <option value="<?php echo $mastDtl['id'].'|'.$mastDtl['sub_head'].'|'.$mastDtl['type']; ?>">
